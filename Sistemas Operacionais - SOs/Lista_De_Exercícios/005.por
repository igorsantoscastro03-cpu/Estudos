programa {
  
  real n1, n2, m
  funcao inicio() {

    escreva("Digite sua primeira nota: ")
    leia(n1)

    escreva("Digite sua segunda nota: ")
    leia(n2)

    m = n1 + n2 / 2

    escreva("Nota 1: ", n1, "\n")

    escreva("Nota 2: ", n2, "\n")

    escreva("Essa é a média de suas notas: ", m)
    
  }
}
