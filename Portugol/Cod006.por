programa {
  cadeia item
  inteiro quant
  //declaração das variáveis
  funcao inicio() { 
    escreva("Nome do item: \n")
    leia(item)// Entrada do nome

    escreva("Quantidade: \n")
    leia(quant)// Entrada do número

    escreva("Item: ",item,", Quantidade em estoque: ", quant)//Saída das variáveis
  }
}
